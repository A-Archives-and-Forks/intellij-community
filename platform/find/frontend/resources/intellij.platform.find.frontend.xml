<idea-plugin package="com.intellij.platform.find.frontend">
  <dependencies>
    <module name="intellij.platform.frontend"/>
    <module name="intellij.platform.find"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">

    <!--suppress PluginXmlRegistrationCheck -->
    <applicationService serviceInterface="com.intellij.find.impl.FindInProjectExecutor"
                        serviceImplementation="com.intellij.platform.find.frontend.FindInProjectExecutorImpl" overrides="true"/>
  </extensions>
</idea-plugin>